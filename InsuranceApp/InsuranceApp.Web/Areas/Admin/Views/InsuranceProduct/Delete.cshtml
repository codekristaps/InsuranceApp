@model InsuranceProduct

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2>
                Delete Insurance Product
            </h2>
        </div>
    </div>
    <div>
        <form method="post">

            <div>
                <div class="pb-3">
                    <label asp-for="Name" class="p-0"></label>
                    <input asp-for="Name" disabled class="form-control" />
                </div>
                <div class="row pb-3">
                    <div class="col-6 col-md-3">
                        <button type="submit" class="btn btn-danger form-control">Delete</button>
                    </div>
                    <div class="col-6 col-md-3">
                        <a asp-controller="InsuranceProduct" asp-action="Index" class="btn btn-outline-secondary form-control border">Back To List</a>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>